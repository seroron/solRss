<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="UTF-8">
    <title>solRess</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimal-ui" />
    <meta name="apple-mobile-web-app-status-bar-style" content="yes" />
    <link rel="stylesheet" href="mobile-angular-ui/dist/css/mobile-angular-ui-base.min.css" />
    <link rel="stylesheet" href="mobile-angular-ui/dist/css/mobile-angular-ui-desktop.min.css" />
    <link rel="stylesheet" href="/css/style.css" />
    <script src="angular/angular.min.js"></script>
    <script src="angular-resource/angular-resource.min.js"></script>
    <script src="mobile-angular-ui/dist/js/mobile-angular-ui.min.js"></script>
    <script src="underscore/underscore-min.js"></script>
    <script src="/js/solrss.js"></script>
  </head>
  <body ng-app="app">
    <div class="app">

      <div class="navbar navbar-app navbar-absolute-top">
        <div class="btn-group justified">
          <a href="#" class="btn btn-navbar">2014/12/31</a>
          <a href="#" class="btn btn-navbar"><i class="fa fa-github fa-navbar"></i> Sources</a>
          <a href="#" class="btn btn-navbar"><i class="fa fa-exclamation-circle fa-navbar"></i> Issues</a>
        </div>
      </div>

      <div class="app-body">
        <div class='app-content'>
          <ng-view class="app-content">

            <% if (typeof notice !== 'undefined') { %>
              <div><%= notice %></div>
              <% } %>
              <div id="main">
                <%- body %>
              </div>
          </ng-view>
        </div>
      </div>
    </div>

    
  </body>
</html>
