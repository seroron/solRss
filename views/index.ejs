<div data-role="page">
  <div data-role="header">
    <h1>SolRss</h1>
    <!-- <div data-role="navbar" data-iconpos="left"> -->
    <!--   <ul> -->
    <!--     <li><a href="#" data-icon="gear">Menu</a></li> -->
    <!--   </ul> -->
    <!-- </div> -->
  </div>

  <div data-role="content">
    <ul data-role="listview" data-inset="true">
      <% for(var dateitem in items) { %>
      <li data-role="list-divider">
        <%= dateitem %>
        <span class="ui-li-count"><%= items[dateitem].length %></span>
      </li>
      <% items[dateitem].forEach(function(i) { %>
      <li><a href="<%= i.link %>" target="_blank">
          <h1 style="white-space:normal"><%= i.title %></h1>
          <p>
            <%= i.date.getHours() %>時<%= i.date.getMinutes() %>分　|　
            <%= i.siteTitle %>
          </p>
      </a></li>
      <% }) %>
      <% } %>
    </ul>
  </div>
</div>


